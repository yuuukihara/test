package model;

public class Hikulogic {
	public double execute(String[] hiku){

		double total = 0;
		for(String result:hiku){
			if(total == 0){
				double a = Integer.parseInt(result);
				total += a;
			}else{
				double b = Integer.parseInt(result);
				total -= b;
			}
		}
		return total;
	}
}
