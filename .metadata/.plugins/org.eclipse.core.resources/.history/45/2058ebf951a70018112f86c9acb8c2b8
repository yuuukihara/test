package fefeaeijfoif;

public class PoisonMatango extends Matango{

	private int attackCount = 5;

	public void attack(Hero h){
		super.attack(h);;
		if(this.attackCount <= 0 ){
			System.out.println("胞子をばら撒いた");
			int hhp = this.hp/5;
			this.hp= hhp;
			System.out.println(hhp+"のダメージ");
			attackCount--;
		}
	}

}
