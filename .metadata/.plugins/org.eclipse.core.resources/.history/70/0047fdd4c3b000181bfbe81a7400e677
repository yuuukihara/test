package model;

public class OturiLogic {
	public double oturi(String[] oturi){
		int first = 0;
		int itemPrice = 0;

		for(String itempay:oturi){
			int a = Integer.parseInt(itempay);
			if(first==0){
				int itemPrice = a;
				first++;
			}else{
				int pay = a;
				int oturi = pay-itemPrice;

				int nowPrice = 0;

				int itiman=0;
				int gosen=0;
				int sen=0;
				int ghyaku=0;
				int hyaku=0;
				int goju=0;
				int ju=0;
				int go=0;
				int iti=0;

				for(int a=0; a< 10;a++){
					if(oturi>=10000){
						itiman += 1;
						oturi-=10000;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=5000){
						gosen += 1;
						oturi-=5000;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=1000){
						sen += 1;
						oturi-=1000;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=500){
						ghyaku += 1;
						oturi-=500;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=100){
						hyaku += 1;
						oturi -=100;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=50){
						goju += 1;
						oturi -= 50;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=10){
						ju += 1;
						oturi-=10;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=5){
						go += 1;
						oturi-=5;
					}
				}
				for(int a=0; a< 10;a++){
					if(oturi>=1){
						iti += 1;
						oturi -= 1;
					}
				}
			}

		}
			}
		}
	}

}
