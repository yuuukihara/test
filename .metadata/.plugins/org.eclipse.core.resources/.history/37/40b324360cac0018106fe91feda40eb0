package servlet;

import java.io.Serializable;

public class User implements Serializable{
	private String id;
	private String name;
	private String pass;

	public User(){}

	public User(String id,String name,String pass){
		this.id = id;
		this.name = name;
		this.pass = pass;
	}

	public String getId(){
		return this.id;
	}
	public String getName(){
		return this.name;
	}
	public String getPass(){
		return this.pass;
	}

}
